1*AUTEUR: PASCAL BOURGUIGNON
2*DESCRIPTION: PROGRAMME DE TEST DU PARSEUR.
3*CE PROGRAMME N'EST PAS SEMANTIQUEMENT VALIDE.
10 AFFICHER 'CE PROGRAMME NE PEUT PAS S''EXECUTER, C''EST  JUSTE POUR TESTER LE PARSEUR.'
11 TERMINER
12 CHAINE CH
14 CHAINE CH11,CH12
16 CHAINE CH21,CH22,CH23,CH24
18 TABLEAU TAB1[10]
20 TABLEAU TAB2[3,2],TAB3[3,3]
22 TABLEAU TAB4[2,4],TAB5[3,5],TAB6[4,6]
24 CHAINE STR;CHAINE STR11,STR12;CHAINE STR21,STR22,STR23,STR24
26 TABLEAU TAB1[10];TABLEAU TAB2[3,2],TAB3[3,3];TABLEAU TAB4[2,4],TAB5[3,5],TAB6[4,6]
28 LIBERER CH
30 LIBERER CH21,CH22,TAB1,TAB4,TAB5
32 LIBERER CH;LIBERER CH21,CH22,TAB1,TAB4,TAB5
34 A_0
36 B_1+A
38 TAB1[A]_42;TAB2[A,B]_4.2
40 LIRE A;LIRE B,TAB1,TAB2[A,B]
42 AFFICHER 'BONJOUR!'
44 AFFICHER['BONJOUR!']
46 AFFICHER[U]'BONJOUR!'
48 AFFICHER[/,'BONJOUR!',/]
50 AFFICHER[/,U,/]'BONJOUR!'
52 AFFICHER[/,3U,/]'BONJOUR!','LE','MONDE'
54 AFFICHER[/,4X,'***',/,3X,'****',/,2X,'*****',/]
56 AFFICHER[/,'HELLO WORLD',C,'BONJOUR MONDE',/]
58 AFFICHER[/,2F6.2]TAB1,TAB2
60 AFFICHER[/,F6.2,E6.2]TAB1,TAB2
62 AFFICHER[/,*X,'***']B*2
64 ALLER EN 10
66 ALLER EN @+3
68 ALLER EN 20;ALLER EN @+3
70 SI A=B ALORS AFFICHER 'EGAL' SINON AFFICHER 'DIFFERENT'
72 SI A=B ALORS DEBUT A_A-1;AFFICHER 'EGAL' FIN SINON DEBUT B_B+1;AFFICHER 'DIFFERENT' FIN
74 PAUSE
76 PAUSE;A_3;B_4
78 FAIRE @+3 POUR NR_1 PAS 2 JUSQUA 10
79 CHAINE FIC;FIC_'ESSAI'
80 GARER     TAB1,NR,FIC
82 CHARGER   TAB0,NR,FIC
83 CHARGER   TAB0,NR,FIC,STAT
84 SUPPRIMER FIC,NR
86 FAIRE @+1 POUR I_1 PAS 3 TANT QUE I<10
88 EXECUTER  'SUBPG',10
90 TERMINER
92 PROCEDURE &TEST1(A,B) LOCAL A,B; AFFICHER[/,U,'(',U,',',U,')',/]'&TEST1',A,B
94 RESULTAT A+B
96 PROCEDURE &TEST2(A,B,C) LOCAL B,C; AFFICHER[/,U,'(',U,',',U,')',/]'&TEST2',B,C
98 A_B+C;RETOUR
100 PROCEDURE &TEST0();AFFICHER[/,U,'()',/]'&TEST0';RETOUR EN 10
102 &TEST0();C_&TEST1(A,B);&TEST2(C,A,B)
104 C_3+&TEST1(A+1,2+B);&TEST2(C,2*A,B*3)
106 C_3-&TEST1(A-1,2-B);&TEST2(C,2/A,B/3)
108 C_3^&TEST1(A^1,2^B);&TEST2(C,2^A,B^3)
110 SI A#B  ET A=B ALORS C_0
112 SI A<B  ET A>B ALORS C_0
114 SI A<=B OU A>=B ALORS C_0
116 SI NON A=B ALORS C_0
118 C_A^B;C_ENT(1/C);C_-C;C_ABS(C);C_EXP(A+C)
120 C_SIN(B+C);C_COS(A+C);C_ATG(A+B+C)
122 C_RAC(ABS(C));C_LGN(C)
124 C_ALE(0)+ALE(10)
126 C_TEM()+ATT();C_ETL(OUL(A,C),OUL(B,C));C_OUX(C,255)
128 CHAINE U,V,W;U_'ONE';V_'TWO'
130 SI U#V  ET U=V ALORS C_0
132 SI U<V  ET U>V ALORS C_0
134 SI U<=V OU U>=V ALORS C_0
136 SI NON U=V ALORS C_0
138 W_U!'-'!V;W_W!'/'!W
140 C_LGR(W)
142 C_POS(W,1,V);A_EQN('A');CH_EQC(A)
144 CH_CCA(42.13);A_CNB(CH,1)
146 CH11_SCH(W,3,6)
148 CH12_SCH(W,3,V)
150 A_SKP(W,1,B)
152 A_PTR(W,1,B)
154 A_GRL(CH,1)
156 B_DAT()
158 TERMINER
