
(:NAME "NUMERO-DE-LIGNE" :PACKAGE "COM.INFORMATIMAGO.LSE" :IDENTIFIER-START-CHARS NIL
 :IDENTIFIER-CONTINUE-CHARS NIL :INTERN-IDENTIFIER NIL :STRING-DELIMITER NIL :SYMBOL-DELIMITER NIL
 :CASE-SENSITIVE T :GRAMMAR "null-grammar" :DOMAIN NIL :DOMAIN-FILE "numero-de-ligne-domain"
 :WHITE-SPACE (#\ ) :LEX-CATS ((TOK-NUMERO "{NUMERO}"))) 
(DEFRULE NUMERO-DE-LIGNE := (TOK-NUMERO) :BUILD
 (PROGN
  (UNLESS (VALID-LINE-NUMBER (NUMERO-VALEUR TOK-NUMERO))
   (ERROR "NUMERO DE LIGNE INVALIDE: ~A" (TOKEN-TEXT TOK-NUMERO)))
  TOK-NUMERO)) 