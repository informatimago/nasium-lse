
(:NAME "UN-FICHIER-ET-DEUX-NUMEROS" :PACKAGE "COM.INFORMATIMAGO.LSE" :IDENTIFIER-START-CHARS "" :IDENTIFIER-CONTINUE-CHARS "" :INTERN-IDENTIFIER NIL :STRING-DELIMITER #\" :SYMBOL-DELIMITER #\' :CASE-SENSITIVE T :GRAMMAR "null-grammar" :DOMAIN NIL :DOMAIN-FILE "un-fichier-et-deux-numeros-domain" :WHITE-SPACE (#\ ) :LEX-CATS ((TOK-VIRGULE ",") (TOK-NUMERO "{NUMERO}") (TOK-IDENTIFICATEUR "{IDENTIFICATEUR}"))) 
(DEFRULE UN-FICHIER-ET-DEUX-NUMEROS := (IDENT TOK-VIRGULE DEUX-NUMEROS) :BUILD (CONS IDENT DEUX-NUMEROS)) 
(DEFRULE IDENT := (TOK-IDENTIFICATEUR) :BUILD (PROGN (UNLESS (AND (CHAR= (CHARACTER "&") (AREF (TOKEN-TEXT TOK-IDENTIFICATEUR) 0)) (<= (LENGTH (TOKEN-TEXT TOK-IDENTIFICATEUR) 5))) (ERROR "IDENTIFICATEUR INVALIDE: ~A" (TOKEN-TEXT TOK-IDENTIFICATEUR))) TOK-IDENTIFICATEUR)) 
(DEFRULE DEUX-NUMEROS := (TOK-NUMERO.1 TOK-VIRGULE TOK-NUMERO.2) :BUILD (PROGN (LIST TOK-NUMERO.1 TOK-NUMERO.2))) 